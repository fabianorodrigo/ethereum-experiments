    "@types/readline-sync": "^1.4.4",
    "dotenv": "^16.0.3",
    "hardhat": "^2.12.0",
    "js-yaml": "^4.1.0",
    "mocha-junit-reporter": "^2.1.0",
    "mocha-multi-reporters": "^1.5.1",
    "solidity-coverage": "^0.8.2"
  },
  "dependencies": {
